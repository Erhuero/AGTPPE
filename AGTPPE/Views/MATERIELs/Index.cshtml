@model IEnumerable<AGTPPE.Models.MATERIEL>

@{
    ViewBag.Title = "Index";
}

<div class="jumbotron">
    <p class="lead">Application de gestion de tickets.</p>

</div>

<div class="navbar-collapse collapse">
    <ul class="nav navbar-nav">

        <li>@Html.ActionLink("Incidents en cours", "IncidentsEnCours", "AffichageIncidents")</li>
        <li>@Html.ActionLink("Incidents clôturés", "Administrateur", "Home")</li>
        <li>@Html.ActionLink("Inventaire", "Create", "MATERIELs")</li>
        <li>@Html.ActionLink("Déclarez un incident", "IncidentsAdmin", "Home")</li>
        <li>@Html.ActionLink("Liste de l'inventaire", "Index", "MATERIELs")</li>
        <li>@Html.ActionLink("Deconnexion", "Index", "Home")</li>
    </ul>
</div>

<h4>Le stock de l'entrepôt</h4>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.emplacementMateriel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.typeMateriel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.modeleMateriel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ipMateriel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.typeConnexionMateriel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.typeIpMateriel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.numInfologMateriel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.celluleMateriel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.etatMateriel)
        </th>
       
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.emplacementMateriel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.typeMateriel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.modeleMateriel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ipMateriel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.typeConnexionMateriel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.typeIpMateriel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.numInfologMateriel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.celluleMateriel)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.etatMateriel)
            </td>
          
            <td>
                @Html.ActionLink("Editer", "Edit", new { id = item.numeroSerieMateriel })
                @Html.ActionLink("Supprimer", "Delete", new { id = item.numeroSerieMateriel })
            </td>
        </tr>
    }

</table>
